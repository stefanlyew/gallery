<h1>Listing artworks</h1>

<table>
  <tr>
    <th>Album</th>
    <th>Title</th>
    <th>Year</th>
    <th>Type</th>
    <th>Medium</th>
    <th>Size</th>
    <th>Price</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artworks.each do |artwork| %>
  <tr>
    <td><%= artwork.album %></td>
    <td><%= artwork.title %></td>
    <td><%= artwork.year %></td>
    <td><%= artwork.type %></td>
    <td><%= artwork.medium %></td>
    <td><%= artwork.size %></td>
    <td><%= artwork.price %></td>
    <td><%= artwork.notes %></td>
    <td><%= link_to 'Show', artwork %></td>
    <td><%= link_to 'Edit', edit_artwork_path(artwork) %></td>
    <td><%= link_to 'Destroy', artwork, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Artwork', new_artwork_path %>
