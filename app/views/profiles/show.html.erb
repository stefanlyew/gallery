	 <script>
  $(function() {
    $( "#accordion" ).accordion({
      collapsible: true,
      active: false
    });
  });
  </script>

  <br/>
	<br/>
	<div class="row">
		<div class="span12">
			<h4 class="profile"><%= @profile.quote %></h4>
		</div>
	</div>
	<br/>
	<div class="row">
		<div class="span12">
        <% if @profile.photo? %>
          <%= image_tag @profile.photo %>
        <% else %>
         <img src="http://placehold.it/1200x300" alt="" />
        <% end %>
		</div>
	</div>
	<br/>
	<div class="span6">
		<p><%= @profile.statement %></p>
	</div>
	<div class="span5">
    <div id="accordion">
      <h4 class="profile">Ongoing and Upcoming Shows</h4>
      <div>
        <p><%= @profile.news %></p>
      </div>
      <h4 class="profile">Recent Exhibits</h4>
      <div>
        <p><%= @profile.recent_exhibits %> </p>
      </div>
      <h4 class="profile">Bibliography/Essays</h4>
      <div>
        <p><%= @profile.bibliography %></p>
      </div>
      <h4 class="profile">Contact Info</h4>
      <div>
        <p><%= @profile.contact %></p>
      </div>
      <h4 class="profile">Resume (PDF)</h4>
    </div>
    
  </div>
</div>
